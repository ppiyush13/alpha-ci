
{
    "auth": {
        "auth-memory": {
            "users": {
                "volte": {
                    "name": "volte",
                    "password": "pass1234"
                }
            }
        }
    },
    "uplinks": {
        "npmjs": {
            "url": "https://registry.npmjs.org/"
        }
    },
    "store": {
        "memory": {
            "limit": 1000
        }
    },
    "logs": [
        {
            "type": "stdout", 
            "format": "pretty",
            "level": "debug"
        }
    ],
    "packages": {
        "volte": {
            "publish": "$authenticated",
            "access": "$authenticated"
        },
        "npm-cli-login": {
            "access": "$anonymous",
            "proxy": "npmjs"
        },
        "demo-pkg": {
            "access": "$anonymous",
            "publish": "$authenticated"
        },
        "**": {
            "access": "$all",
            "publish": "$authenticated",
            "unpublish": "$authenticated",
            "proxy": "npmjs"
        }
    }
}
