{
    "name": "volte",
    "version": "0.0.0",
    "description": "Robust, simple replacement for npm publish",
    "license": "MIT",
    "scripts": {
        "test": "jest",
        "e": "node -r @babel/register ./e2e/e2e.test.js",
        "et": "nyc npm run e",
        "e2e": "node -r esm ./e2e/e2e.test.js",
        "e2e:coverage": "nyc --cache false npm run e2e",
        "e2e8": "c8 npm run e2e",
        "local": "verdaccio --config ./verda-conf/config.yml",
        "server": "verdaccio --config ./e2e/verdaccio-local/verdaccio-config.json",
        "run": "node -r esm ./e2e/runner.js"
    },
    "bin": {
        "volte": "bin/volte.js"
    },
