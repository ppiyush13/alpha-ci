{
    "auth": {
        "auth-memory": {
            "users": {
                "alpha": {
                    "name": "alpha",
                    "password": "Alpha1234"
                }
            }
        }
    },
    "uplinks": {
        "npmjs": {
            "url": "https://registry.npmjs.org/"
        }
    },
    "store": {
        "memory": {
            "limit": 1000
        }
    },
    "listen": [
        "localhost:13130"
    ],
    "packages": {
        "**": {
            "access": "$anonymous",
            "publish": "$authenticated",
            "unpublish": "$authenticated",
            "proxy": "npmjs"
        }
    }
}
